<?xml version="1.0"?>

<schema xmlns="http://www.concrete5.org/doctrine-xml/0.5"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 https://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd">
	<table name="btAdum">
		<field name="bID" type="integer">
			<unsigned />
			<key />
		</field>
		<field name="filter" type="text">
			<default value="" />
		</field>		
		<field name="parsing" type="text">
			<default value="-1" />
		</field>

		<field name="year" type="integer">
			<unsigned />
			<default value="2016" />
		</field>

		<field name="details" type="text">
			<default value="True"/>
		</field>
		<field name="langage" type="text">
			<default value="FR"/>
		</field>
	</table>
</schema>
