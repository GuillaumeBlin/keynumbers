<?xml version="1.0"?>

<schema xmlns="http://www.concrete5.org/doctrine-xml/0.5"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 https://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd">
	<table name="btKeyNumbers">
		<field name="bID" type="integer">
			<unsigned />
			<key />
		</field>
		<field name="nbMaxPerRow" type="integer">
			<unsigned />
			<default value="3" />
		</field>
		<field name="keyValues" type="text">
			<default value="" />
		</field>		
		<field name="keyDescriptions" type="text">
			<default value="" />
		</field>
	</table>
</schema>
